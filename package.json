{
  "name": "awell-orchestration-stories",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "ts": "tsc --noEmit --incremental",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "npm run generate-types && npm run ts && next lint --fix",
    "generate-types": "graphql-codegen --config codegen.js",
